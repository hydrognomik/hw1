{
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "Hls": false,
    "Uint8Array": false
  },
  "rules": {
    "newline-after-var": "error",
    "arrow-body-style": ["error", "as-needed"],
    "arrow-parens": ["error", "as-needed"],
    "semi": "error",
    "indent": ["error", 2, { "SwitchCase": 1 }]
  }
}
